{
  "name": "serialize-ts",
  "version": "2.5.1",
  "description": "Serializers for typescript",
  "main": "dist/index.js",
  "scripts": {
    "commit": "npx git-cz",
    "test": "./node_modules/karma/bin/karma start karma.conf.js",
    "test-once": "./node_modules/karma/bin/karma start karma.conf.js --single-run",
    "compile": "./node_modules/typescript/bin/tsc -p ./tsconfig.json",
    "format": "prettier-tslint fix '**/*.ts' ",
    "tslint": "./node_modules/tslint/bin/tslint -p ./tslint.json"
  },
  "keywords": [
    "typescript",
    "serialize",
    "serialization",
    "deserialization",
    "converters",
    "json",
    "http"
  ],
  "author": "xoll",
  "license": "ISC",
  "dependencies": {
    "reflect-metadata": "^0.1.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AndreyZelenskiy/serialize.git"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.4",
    "es6-shim": "^0.35.4",
    "jasmine": "^2.9.0",
    "jasmine-core": "^2.9.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-typescript": "^3.0.9",
    "prettier": "^1.15.3",
    "prettier-tslint": "^0.4.0",
    "tslint": "^5.12.1",
    "typescript": "^2.6.2"
  }
